{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "brnd",
  "homepage": "https://registry.brndhub.dev/",
  "items": [
    {
      "name": "brnd",
      "type": "registry:style",
      "cssVars": {
        "light": {
          "primary": "oklch(0.63 0.17 149)",
          "primary-foreground": "oklch(0.985 0 0)",
          "radius": "0.825rem"
        },
        "dark": {
          "primary": "oklch(0.63 0.17 149)",
          "primary-foreground": "oklch(0.985 0 0)"
        }
      },
      "files": [
        {
          "path": "registry/brnd/rules/registry.mdc",
          "type": "registry:file",
          "target": ".cursor/rules/registry.mdc"
        }
      ]
    },
    {
      "name": "login-form",
      "type": "registry:block",
      "title": "Login Form",
      "description": "A login form with email and password fields.",
      "files": [
        {
          "path": "registry/brnd/auth/login-form.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
